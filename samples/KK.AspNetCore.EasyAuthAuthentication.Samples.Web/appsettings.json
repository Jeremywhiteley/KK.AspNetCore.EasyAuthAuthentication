{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "easyAuthOptions": {
    "AuthEndpoint": ".auth/me",
    "providerOptions": [
      {
        "ProviderName": "EasyAuthForApplicationsService",
        "Enabled": true
      },
      {
        "ProviderName": "EasyAuthWithHeaderService",
        "Enabled": true
      }
    ]
  }
}
